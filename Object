 // 1.  valueChangeObject

const person = {
    Nmae: 'Kamal',
    FullName: 'kamla khan',
    Age: 25,
    Village: 'Mymenshing',
    PostCode: 2000
}
// console.log(person)
person.Age = 26;
console.log(person.Age)

// 2. accessObjectKeys
const person = {
    Name: 'Kamal',
    FullName: 'kamla khan',
    Age: 25,
    Village: 'Mymenshing',
    PostCode: 2000
}

const accessKeysObject = Object.keys(person)
console.log(accessKeysObject)

// 3. accessObjectvalues

const person = {
    Name: 'Kamal',
    FullName: 'kamla khan',
    Age: 25,
    Village: 'Mymenshing',
    PostCode: 2000
}

const accessValueObject = Object.values(person)
console.log(accessValueObject)

//4. loop using Object


const person = {
    Name: 'Kamal',
    FullName: 'kamla khan',
    Age: 25,
    Village: 'Mymenshing',
    PostCode: 2000
}

if ("Name" in person) {
    console.log("Name exists in the object");
}
else {
    console.log("Name does not exist in the object");
}
